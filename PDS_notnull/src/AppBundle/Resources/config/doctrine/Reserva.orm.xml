<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Reserva" table="reserva">
    <indexes>
      <index name="fk_id_auto" columns="id_auto"/>
      <index name="id_lugar" columns="id_lugar"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idAuto" type="integer" column="id_auto" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idLugar" type="integer" column="id_lugar" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fechaDesde" type="datetime" column="fecha_desde" nullable="false"/>
    <field name="fechaHasta" type="datetime" column="fecha_hasta" nullable="true"/>
  </entity>
</doctrine-mapping>
